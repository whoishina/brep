{
  "name": "bun-fullstack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "brep": "bun .brep/brep",
    "prisma:generate": "prisma generate",
    "ui:dev": "vite",
    "ui:preview": "vite preview",
    "ui:build": "tsc -b && vite build --mode production",
    "ui:start": "serve -s dist/html -p 5173",
    "lint": "eslint .",
    "app:dev": "bun --watch src/app/index.ts",
    "app:bridge": "bun --bun --watch src/bridge.ts",
    "app:build": "bun build src/app/index.ts --target bun --outdir ./dist/app",
    "app:start": "bun --bun dist/app/index.js",
    "dev": "concurrently \"bun --bun run app:dev\" \"bun --bun run ui:dev\"",
    "start": "concurrently \"bun run app:start\" \"bun run ui:start\"",
    "build": "bun prisma:generate && concurrently \"bun --bun run app:build\" \"bun --bun run ui:build\""
  },
